ALWAYS_EMBED_SWIFT_STANDARD_LIBRARIES = NO
FRAMEWORK_SEARCH_PATHS = $(inherited) "$PODS_CONFIGURATION_BUILD_DIR/Artsy+UIColors" "$PODS_CONFIGURATION_BUILD_DIR/Artsy+UIFonts" "$PODS_CONFIGURATION_BUILD_DIR/Artsy-UIButtons" "$PODS_CONFIGURATION_BUILD_DIR/UIView+BooleanAnimations"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$PODS_CONFIGURATION_BUILD_DIR/Artsy+UIColors/Artsy_UIColors.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/Artsy+UIFonts/Artsy_UIFonts.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/Artsy-UIButtons/Artsy_UIButtons.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/UIView+BooleanAnimations/UIView_BooleanAnimations.framework/Headers"
OTHER_LDFLAGS = $(inherited) -framework "Artsy_UIButtons" -framework "Artsy_UIColors" -framework "Artsy_UIFonts" -framework "UIView_BooleanAnimations"
PODS_BUILD_DIR = $BUILD_DIR
PODS_CONFIGURATION_BUILD_DIR = $PODS_BUILD_DIR/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_ROOT = ${SRCROOT}/Pods
